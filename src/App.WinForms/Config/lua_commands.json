{
  "playerCheck": "player_checker('{name}')",
  "monsterRegenerate": "//regenerate {monsterId} {count}",
  "monsterAddNpcAtPlayer": "add_npc( gv('x','{playerName}'), gv('y','{playerName}'),{monsterId},{count},{minutesLifetime}, gv('layer','{playerName}'))",
  "monsterAddNpcAtCoords": "add_npc( {x},{y},{monsterId},{count},{minutesLifetime},{layer})",
  "insertItemSelf": "insert_item({itemId},{amount},{enhance},{level},{statusFlag})",
  "insertItemPlayer": "insert_item({itemId},{amount},{enhance},{level},{statusFlag},'{playerName}')",
  "setWearItemLevelOwn": "set_item_level(get_wear_item_handle({wearSlot}),{level})",
  "setWearItemEnhanceOwn": "set_item_enhance(get_wear_item_handle({wearSlot}),{enhance})",
  "setWearItemAppearanceOwn": "set_item_appearance_code(get_wear_item_handle({wearSlot}),{itemCode})",
  "changeWearItemCodeOwn": "change_item_code(get_wear_item_handle({wearSlot}),{itemCode})",
  "setWearItemLevelPlayer": "set_item_level(get_wear_item_handle({wearSlot},'{playerName}'),{level})",
  "setWearItemEnhancePlayer": "set_item_enhance(get_wear_item_handle({wearSlot},'{playerName}'),{enhance})",
  "setWearItemAppearancePlayer": "set_item_appearance_code(get_wear_item_handle({wearSlot},'{playerName}'),{itemCode})",
  "changeWearItemCodePlayer": "change_item_code(get_wear_item_handle({wearSlot},'{playerName}'),{itemCode})",
  "learnSkill": "learn_skill({skillId})",
  "learnSkillForPlayer": "learn_skill({skillId},'{playerName}')",
  "setSkill": "set_skill({skillId},{level})",
  "setSkillForPlayer": "set_skill({skillId},{level},'{playerName}')",
  "learnAllSkill": "learn_all_skill('{playerName}')",
  "learnCreatureAllSkill": "learn_creature_all_skill({slotIndex})",
  "learnCreatureAllSkillForPlayer": "learn_creature_all_skill({slotIndex},'{playerName}')",
  "removeSkill": "remove_skill({skillId})",
  "removeSkillForPlayer": "remove_skill({skillId},'{playerName}')",
  "resetSkilltreeForPlayer": "reset_skilltree({jobDepth},'{playerName}')",
  "resetSummonSkillForPlayer": "reset_summon_skill({handleOrIndex},{forceReset},'{playerName}')",
  "learnCreatureSkillSelf": "creature_learn_skill({skillId}, gcv(get_creature_handle(0), \"handle\"))",
  "learnCreatureSkill": "creature_learn_skill({skillId}, gcv(get_creature_handle(0), \"handle\"), '{playerName}')",
  "castWorldState": "cast_world_state({stateId},{level},{durationMinutes}*100*60)",
  "addEventState": "add_event_state({stateId},{level})",
  "removeEventState": "remove_event_state({stateId},get_state_level({stateId}))",
  "addPlayerState": "add_state({stateId},{level},{durationMinutes}*100*60,'{playerName}')",
  "removePlayerState": "remove_state({stateId},get_state_level({stateId},'{playerName}'),'{playerName}')",
  "addCreatureState": "add_cstate({stateId},{level},{durationMinutes}*100*60,'{playerName}')",
  "removeCreatureState": "remove_cstate({stateId},get_state_level({stateId},'{playerName}'),'{playerName}')",
  "moveToNpc": "move_to_npc({npcId}, {x}, {y}, {layer})",
  "spawnSummon": "spawn_summon({summonId}, {amount})"
}
